{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    },
    "typeRoots": ["./node_modules/@types", "./types"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/docs/ResumeList.jsx", "app/api/chat/route.js", "components/docs/edit/DocsToolBarCompo/TextAlignButtons.jsx", "components/docs/edit/DocsEditNew.jsx", "components/docs/edit/DocsToolBarCompo/LinkButtons.jsx", "components/docs/edit/MenuBar.jsx", "app/test/MenuBar.jsx", "app/test/extension.js", "app/test1/page.jsx", "app/test/page.jsx"],
  "exclude": ["node_modules"]
}
